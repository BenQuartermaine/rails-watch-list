<div class="p-5">
<div class="d-flex justify-content-between align-items-center">
  <h1 class="display-1 text-center">Listing Lists</h1>
  <div>
    <%= link_to 'Add List', new_list_path, class: 'btn btn-primary' %>
  </div>
</div>

  <div class="py-4">
    <% @lists.each do |l| %>

    <p>
      <%= link_to l.name, list_path(l) %>
    </p>

    <% end %>
  </div>
</div>

